<!-- This is for the Angular 1 reboot of the UI 2017 style -->
<!DOCTYPE html>
<html ng-app="uiApp">
<head>
    <title><%= typeof title == 'undefined' ? 'ourglass' : title %></title>
    <link rel="shortcut icon" type="image/png" href="images/favicon-16x16.png"/>

    <!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="theme-color" content="#006176">

    <!-- Grab the common JS libs -->
    <%- partial( '../partials/ui2/jsdeps' ) %>

    <!-- Grab the common CSS  -->
    <%- partial( '../partials/ui2/cssdeps' ) %>

    <!-- Import your app, controllers, etc. here. Path needs to be out of /assets/js/views/  -->
    <!-- This means a src like src="/js/views/ui". Bower components are pulled from /bower_components/... -->

    <script src="/ui2app/app/ui2App.module.js"></script>
    <script src="/ui2app/app/ui2App.routes.js"></script>

    <script src="/ui2app/app/services/bellini/apicore.service.js"></script>
    <script src="/ui2app/app/services/bellini/auth.service.js"></script>
    <script src="/ui2app/app/services/bellini/users.service.js"></script>
    <script src="/ui2app/app/services/bellini/geocode.service.js"></script>
    <script src="/ui2app/app/services/bellini/media.service.js"></script>


    <script src="/ui2app/app/services/bellini/user-auth.service.js"></script>
    <script src="/ui2app/app/services/ui/ui.service.js"></script>

    <script src="/ui2app/app/components/navside/navside.controller.js"></script>
    <script src="/ui2app/app/components/navtop/navtop.controller.js"></script>
    <script src="/ui2app/app/components/admin/admin.controller.js"></script>
    <script src="/ui2app/app/components/user/userbasicinfo.component.js"></script>
    <script src="/ui2app/app/components/user/userauth.component.js"></script>

    <script src="/ui2app/app/components/account/myaccount.controller.js"></script>

    <script src="/ui2app/app/components/user/ringbadge.component.js"></script>
    <script src="/ui2app/app/components/uicomponents/dragAndDrop/dragAndDrop.directive.js"></script>
    <script src="/ui2app/app/components/uicomponents/ImgInput/imageinput.directive.js"></script>
    <script src="/ui2app/app/components/uicomponents/googleMap/googleMap.component.js"></script>


</head>

<body style="background: url(/images/radiotele.jpg); background-size: cover;">

<!-- BODY IMPORT -->

<%- body %>


</body>
</html>